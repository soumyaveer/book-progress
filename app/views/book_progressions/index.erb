<div class="row">
  <div class="col-md-8 col-md-offset-4 col-sm-8 col-sm-offset-4">
    <h3>Your Booklist, <%= current_user.username.capitalize %>!</h3>
    <div>
      <% @book_progressions.each do |book_progression| %>
        <p><a href="/book_progressions/<%= book_progression.id %>"> <%= book_progression.book.title %></a> - <strong><%= book_progression.percent_read %>%</strong> complete</p>
      <% end %>
    </div>
    <br>
    <a class="btn btn-default" href="/book_progressions/new">Add Book</a>
    <a class="btn btn-default" href="/logout">Logout</a>
    <a class="pull-right" href="/">Back</a>
  </div>
</div>
